@layout CoverLayout
@page "/"

<FileContent ContentFilePath="assets/content/indexcontent.html" />

@code {
	[CascadingParameter] CoverLayout Layout { get; set; }

	protected override async Task OnInitializedAsync()
	{
		Layout.SetCover("NSYS SocialGuard", "Security for your Community.", true, true);
		await base.OnInitializedAsync();
	}
}
